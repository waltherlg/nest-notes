generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_DATABASE_URL")
}

model PrismaUser {
  id       String @id @default(uuid()) @db.Uuid
  userName String @unique
  email    String @unique
}
